<% if @folder.partial_name == "window" %>
  $('.folderContainer').html('<%= j render partial: "#{ @folder.partial_name }" %>');
<% else %>
  $('body').append('<%= j render partial: "#{ @folder.partial_name }" %>');
<% end %>

$('.closebtn').on('click', function() {
  console.log('Click');
  $(this).parents('#computer').remove();
})
